<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.itcast.ssh.domain">
<class name="Act_re_model" table="act_re_model">
	<id name="ID_" type="string" column="ID_">
		 <generator class="native"/>
	</id>
	<property name="REV_" type="int" column="REV_" />
	<property name="NAME_" type="string" column="NAME_" />
	<property name="KEY_" type="string" column="KEY_" />
	<property name="CATEGORY_" type="string" column="CATEGORY_" />
	<property name="CREATE_TIME_" type="date" column="CREATE_TIME_" />
	<property name="LAST_UPDATE_TIME_" type="date" column="LAST_UPDATE_TIME_" />
	<property name="VERSION_" type="int" column="VERSION_" />
	<property name="META_INFO_" type="string" column="META_INFO_" />
	<property name="DEPLOYMENT_ID_" type="string" column="DEPLOYMENT_ID_" />
		<property name="EDITOR_SOURCE_VALUE_ID_" type="string" column="EDITOR_SOURCE_VALUE_ID_" />
	<property name="EDITOR_SOURCE_EXTRA_VALUE_ID_" type="string" column="EDITOR_SOURCE_EXTRA_VALUE_ID_" />
	

</class>
</hibernate-mapping>
