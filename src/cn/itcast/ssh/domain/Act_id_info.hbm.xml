<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.itcast.ssh.domain">
	<class name="Act_id_info" table="act_id_info">
		<id name="ID_" type="string" column="ID_">
			<generator class="native" />
		</id>
		<property name="REV_" type="int" column="REV_" />
		<property name="USER_ID_" type="string" column="USER_ID_" />
		<property name="TYPE_" type="string" column="TYPE_" />
		<property name="KEY_" type="string" column="KEY_" />
		<property name="VALUE_" type="string" column="VALUE_" />
		<property name="PASSWORD_" type="java.sql.Blob" column="PASSWORD_" />
		<property name="PARENT_ID_" type="string" column="PARENT_ID_" />
	



	</class>
</hibernate-mapping>

