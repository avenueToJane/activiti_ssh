<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.itcast.ssh.domain">
	<class name="Act_ru_task" table="act_ru_task">
		<id name="ID_" type="string" column="ID_">
			<generator class="native" />
		</id>
		<property name="REV_" type="int" column="REV_" />
		<property name="EXECUTION_ID_" type="string" column="EXECUTION_ID_" />
		<property name="PROC_INST_ID_" type="string" column="PROC_INST_ID_" />
		<property name="PROC_DEF_ID_" type="string" column="PROC_DEF_ID_" />
		<property name="NAME_" type="string" column="NAME_" />
		<property name="PARENT_TASK_ID_" type="string" column="PARENT_TASK_ID_" />
		<property name="DESCRIPTION_" type="string" column="DESCRIPTION_" />
		<property name="TASK_DEF_KEY_" type="string" column="TASK_DEF_KEY_" />
		<property name="OWNER_" type="string" column="OWNER_" />
		<property name="ASSIGNEE_" type="string" column="ASSIGNEE_" />
		<property name="DELEGATION_" type="string" column="DELEGATION_" />
		<property name="PRIORITY_" type="int" column="PRIORITY_" />
		<property name="CREATE_TIME_" type="date" column="CREATE_TIME_" />
		<property name="DUE_DATE_" type="date" column="DUE_DATE_" />
		<property name="SUSPENSION_STATE_" type="int" column="SUSPENSION_STATE_" />

	</class>
</hibernate-mapping>
