<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.itcast.ssh.domain">
<class name="Act_ge_bytearray" table="act_ge_bytearray">
	<id name="id_" type="string" column="ID_">
		 <generator class="native"/>
	</id>
	<property name="rev_" type="int" column="REV_" />
	<property name="name_" type="string" column="NAME_" unique="true"/>
	<property name="deployment_id_" type="string" column="DEPLOYMENT_ID_" />
	<property name="bytes_" type="java.sql.Blob" column="BYTES_" />
	<property name="generated_" type="int" column="GENERATED_" />
</class>
</hibernate-mapping>
