<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping package="cn.itcast.ssh.domain">
<class name="Act_re_deployment" table="Act_re_deployment">
	<id name="ID_" type="string" column="ID_">
		 <generator class="native"/>
	</id>
	<property name="NAME_" type="string" column="NAME_" />
	<property name="CATEGORY_" type="string" column="CATEGORY_" />
	<property name="DEPLOY_TIME_" type="date" column="DEPLOY_TIME_" />
	
	
		
</class>
</hibernate-mapping>
